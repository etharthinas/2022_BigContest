import pandas as pd

session=pd.read_csv("session.csv")
session['sessionlength']=pd.to_timedelta(session.sessionlength)
session.sessionlength=session.sessionlength.dt.total_seconds()
idx=session[session['sessionlength']<=3].index
session=session.drop(idx)
temp=session.groupby(['user_id']).count()
spendedtime=temp[['sessionnum']]
spendedtime['certificate']=session.groupby(['user_id']).sum()['CompleteIDCertification']
spendedtime['totaltime']=session.groupby(['user_id']).sum()['sessionlength']

spendedtime['pyunggyuntime']=spendedtime['totaltime']/spendedtime['sessionnum']
spendedtime
